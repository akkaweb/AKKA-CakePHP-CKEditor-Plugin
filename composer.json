{
    "name": "akkaweb/cakephp-ckeditor",
    "description": "CKEditor plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "cakephp3", "ckeditor"],
    "license": "MIT",
    "authors": [
	{
	    "name": "Andre Santiago",
	    "email": "admin@akkaweb.com"
	}
    ],
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "AkkaCKEditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AkkaCKEditor\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
